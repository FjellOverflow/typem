{
  "name": "typem",
  "version": "0.1.4",
  "private": true,
  "description": "Type out the list items as quickly as possible.",
  "homepage": "https://github.com/FjellOverflow/typem",
  "bugs": {
    "url": "https://github.com/FjellOverflow/typem/issues"
  },
  "license": "GPL-3.0-only",
  "author": "FjellOverflow",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p typecheck \"build-only {@}\" --",
    "preview": "vite build && vite preview",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "typecheck": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "release": "eslint . && prettier --check src && commit-and-tag-version --sign"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tsconfig/node22": "^22.0.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.3.0",
    "@vueuse/router": "^13.3.0",
    "daisyui": "^5.0.43",
    "lucide-vue-next": "^0.515.0",
    "shepherd.js": "^14.5.0",
    "tailwindcss": "^4.1.10",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-i18n": "^11.1.5",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.1",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "commit-and-tag-version": "^12.5.1",
    "cypress": "^14.4.1",
    "eslint": "^9.28.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "^10.2.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.5",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.3",
    "vitest": "^3.2.3",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10"
  }
}
