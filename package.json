{
  "name": "typem",
  "version": "0.1.10",
  "private": true,
  "description": "Type out the list items as quickly as possible.",
  "homepage": "https://github.com/FjellOverflow/typem",
  "bugs": {
    "url": "https://github.com/FjellOverflow/typem/issues"
  },
  "license": "GPL-3.0-only",
  "author": "FjellOverflow",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p typecheck \"build-only {@}\" --",
    "preview": "vite build && vite preview",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "typecheck": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "release": "eslint . && prettier --check src && commit-and-tag-version --sign"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@tsconfig/node22": "^22.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^14.0.0",
    "@vueuse/router": "^14.0.0",
    "caniuse-lite": "^1.0.30001754",
    "daisyui": "^5.5.0",
    "flag-icons": "^7.5.0",
    "lucide-vue-next": "^0.553.0",
    "shepherd.js": "^14.5.1",
    "tailwindcss": "^4.1.17",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.16.1",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-i18n": "^11.1.12",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "commit-and-tag-version": "^12.6.0",
    "cypress": "^15.6.0",
    "eslint": "^9.39.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^10.5.1",
    "jsdom": "^27.2.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.3",
    "vitest": "^4.0.8",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.3"
  }
}
